ompd-tpl:MaintenanceProcedureDocument[
    ottr:IRI ?iri,
    ? xsd:string ?procedure_name,
    ? xsd:string ?hasTitle   
] :: {
    o-rdf:Type(?iri, ompd:MaintenanceProcedureDocument),
    ottr:Triple(?iri, rdfs:label, ?procedure_name),
    o-owl-dec:DatatypeProperty(proc:hasTitle, "hasTitle", ottr:none, ottr:none, ottr:none),
    ottr:Triple(?iri, proc:hasTitle, ?hasTitle)
} .

ompd-tpl:MaintenanceProcedureProcess[
    owl:NamedIndividual ?iri,
    owl:NamedIndividual ?procedure_document_iri,
    ? xsd:string ?procedure_name
] :: {
    o-rdf:Type(?iri, ompd:MaintenanceProcedureProcess),
    ottr:Triple(?iri, rdfs:label, ?procedure_name),
    ottr:Triple(?procedure_document_iri, lis:isAbout, ?iri)
} .

ompd-tpl:MantenanceTask[
    owl:NamedIndividual ?iri,
    owl:Class ?type,
    ? xsd:string ?task_name,
    ? owl:NamedIndividual ?afterMainteanceTaskIri,
    ? owl:NamedIndividual ?parentMaintenanceTaskIri,
    ? List<ottr:IRI> ?differentFrom 
] :: {
    ottr:Triple(ompd:directActivityPartOf, rdf:type, owl:ObjectProperty),
    ottr:Triple(ompd:hasDirectActivityPart, rdf:type, owl:ObjectProperty),
    ottr:Triple(ompd:hasIndirectActivityPart, rdf:type, owl:ObjectProperty),
    ottr:Triple(ompd:indirectActivityPartOf, rdf:type, owl:ObjectProperty),
    o-rdf:Type(?iri, ?type),
    ottr:Triple(?iri, rdfs:label, ?task_name),
    ottr:Triple(?iri, ompd:directlyAfter, ?afterMainteanceTaskIri),
    ottr:Triple(?iri, ompd:directActivityPartOf, ?parentMaintenanceTaskIri),
    cross | ottr:Triple(?iri, owl:differentFrom, ++?differentFrom)
} .

ompd-tpl:MaintenanceTaskClass[
    owl:Class ?type,
    ? List<owl:Class> ?disjointWith
] :: {
    o-rdfs:SubClassOf(?type, ompd:MaintenanceTask),
    o-owl-ax:DisjointClasses(?disjointWith)
} .

ompd-tpl:MantenanceTaskDescription[
    owl:NamedIndividual ?iri,
    ? xsd:string ?task_description_name,
    ? xsd:string ?task_description_text,
    ? owl:NamedIndividual ?denotes
] :: {
    o-rdf:Type(?iri, ompd:MaintenanceTaskDescription),
    ottr:Triple(?iri, rdfs:label, ?task_description_name),
    ottr:Triple(proc:hasDescription, rdf:type, owl:DatatypeProperty),
    ottr:Triple(?iri, rdfs:hasTextValue, ?task_description_text),
    ottr:Triple(?iri, lis:denotes, ?denotes)
} .

ompd-tpl:Resource[
    ottr:IRI ?iri,
    ? xsd:string ?resource_name,
    ? ottr:IRI ?participantIn,
    ? List<owl:Class> ?type
] :: {
    cross | o-rdf:Type(?iri, ++?type),
    ottr:Triple(?iri, rdfs:label, ?resource_name),
    ottr:Triple(?iri, lis:participantIn, ?participantIn)
} .

ompd-tpl:ResourceWithDetails[
    ottr:IRI ?iri,
    ? List<owl:Class> ?type,
    ? ottr:IRI ?participantIn,
    ? xsd:string ?resource_label,
    ? xsd:string ?resource_name,
    ? xsd:string ?hasUnits,
    ? xsd:string ?hasQuantity
] :: {
    ompd-tpl:Resource(?iri, ?resource_label, ?participantIn, ?type),
    ottr:Triple(?iri, proc:hasUnits, ?hasUnits),
    ottr:Triple(?iri, proc:hasQuantity, ?hasQuantity),
    ottr:Triple(?iri, rdfs:label, ?resource_name),
    ottr:Triple(proc:hasUnits, rdf:type, owl:ObjectProperty),
    ottr:Triple(proc:hasQuantity, rdf:type, owl:ObjectProperty)
} .

ompd-tpl:ItemInMaintenanceTask[
    ottr:IRI ?iri,
    ? xsd:string ?name,
    ? ottr:IRI ?participantIn,
    ? List<owl:Class> ?type
] :: {
    cross | o-rdf:Type(?iri, ++?type),
    ottr:Triple(?iri, rdfs:label, ?name),
    ottr:Triple(?iri, lis:participantIn, ?participantIn)
} .

ompd-tpl:Activity[
    ottr:IRI ?iri,
    ? xsd:string ?activity_name,
    ? ottr:IRI ?activity_of,
    ? List<owl:Class> ?type
] :: {
    o-rdfs:SubClassOf(proc:AtomicAction, lis:Activity),
    ottr:Triple(proc:hasAtomicAction, rdf:type, owl:ObjectProperty),
    ottr:Triple(proc:atomicActionIn, rdf:type, owl:ObjectProperty),
    ottr:Triple(proc:AtomicAction, rdf:type, owl:Class),
    o-owl-ax:SubObjectPropertyOf(proc:hasAtomicAction, lis:hasActivityPart),
    o-owl-ax:SubObjectPropertyOf(proc:atomicActionIn, lis:activityPartOf),
    cross | o-rdf:Type(?iri, ++?type),
    ottr:Triple(?iri, rdfs:label, ?activity_name),
    ottr:Triple(?activity_of, proc:hasAtomicAction, ?iri)
} .

ompd-tpl:Hazard[
    owl:NamedIndividual ?iri,
    owl:NamedIndividual ?process_iri,
    ? xsd:string ?hazard_label,
    ? owl:NamedIndividual ?hazard_realisation_process,
    ? xsd:string ?hazard_description
] :: {
    o-rdf:Type(?iri, ompd:HazardInMaintenanceProcess),
    o-rdf:Type(?hazard_realisation_process, ompd:HazardRealizationProcess),
    ottr:Triple(?iri, lis:realizedIn, ?hazard_realisation_process),
    ottr:Triple(?iri, rdfs:label, ?hazard_label),
    ottr:Triple(?iri, proc:hasDescription, ?hazard_description)
} .

ompd-tpl:HasPrimaryParticipant[
    ottr:IRI ?source,
    ottr:IRI ?target
] :: {
    ottr:Triple(?source, proc:hasPrimaryParticipant, ?target),
    o-owl-ax:SubObjectPropertyOf(proc:hasPrimaryParticipant, lis:hasParticipant)
} .