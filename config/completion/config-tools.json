{
  "output_data_folder": "data/output/datafiles",
  "prefix": "proc:",
  "input_data_filepath": "data/input/tools.csv",
  "output_schema": [
    {
      "filename": "tools.csv",
      "columns": [
        {
          "name": "iri",
          "type": "IRI",
          "template": "tool_id"
        },
        {
          "name": "type",
          "type": "LITERAL",
          "template": "ompd:Tool"
        },
        {
          "name": "processIri",
          "type": "IRI",
          "template": "procedure_id"
        },
        {
          "name": "toolLabel",
          "type": "TEXT",
          "template": "tool_name"
        }
      ]
    }
  ]
}
