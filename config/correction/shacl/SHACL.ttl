proc:MaintenanceActionWithNoItem
    a sh:NodeShape ;
    sh:targetClass ompd:MaintenanceAction ; 
    sh:property [           
        sh:path ompd:hasPatient ;    
        sh:minCount 1 ;
    ] ;
    sh:closed false;
    sh:ignoredProperties ( rdf:type owl:topDataProperty owl:topObjectProperty ) ;
.
